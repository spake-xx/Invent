<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_summary">

    <fragment
        android:id="@+id/navigation_summary"
        android:name="com.spake.invent.ui.items.ItemsFragment"
        android:label="@string/title_summary"
        tools:layout="@layout/fragment_items" />

    <fragment
        android:id="@+id/navigation_bags"
        android:name="com.spake.invent.ui.bags.BagsFragment"
        android:label="@string/title_bags"
        tools:layout="@layout/fragment_items" />

    <fragment
        android:id="@+id/navigation_lockers"
        android:name="com.spake.invent.ui.lockers.LockersFragment"
        android:label="@string/title_lockers"
        tools:layout="@layout/fragment_items" />
</navigation>
